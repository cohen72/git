{
	"branches": [
		"+([0-9])?(.{+([0-9]),x}).x",
		"master",
		"next",
		{
			"name": "develop",
			"prerelease": "qa"
		},
		{
			"name": "release",
			"prerelease": "rc"
		},
		{
			"name": "beta",
			"prerelease": true
		}
	],
	"npmPublish": false,
	"plugins": [
		[
			"@semantic-release/commit-analyzer",
			{
				"preset": "angular",
				"releaseRules": [
					{
						"type": "feat",
						"release": "minor"
					},
					{
						"type": "fix",
						"release": "patch"
					},
					{
						"type": "refactor",
						"release": "patch"
					},
					{
						"type": "ci",
						"release": "patch"
					},
					{
						"type": "docs",
						"scope": "README",
						"release": "patch"
					}
				]
			}
		],
		[
			"@semantic-release/release-notes-generator",
			{
				"preset": "angular",
				"presetConfig": {
					"types": [
						{
							"type": "feat",
							"section": "Features"
						},
						{
							"type": "fix",
							"section": "Bug Fixes"
						},
						{
							"type": "perf",
							"section": "Performance Improvements"
						},
						{
							"type": "revert",
							"section": "Reverts"
						},
						{
							"type": "docs",
							"section": "Documentation",
							"hidden": true
						},
						{
							"type": "style",
							"section": "Styles",
							"hidden": true
						},
						{
							"type": "chore",
							"section": "Miscellaneous Chores",
							"hidden": true
						},
						{
							"type": "refactor",
							"section": "Code Refactoring",
							"hidden": false
						},
						{
							"type": "test",
							"section": "Tests",
							"hidden": true
						},
						{
							"type": "build",
							"section": "Build System",
							"hidden": true
						},
						{
							"type": "ci",
							"section": "Continuous Integration",
							"hidden": false
						},
						{
							"type": "improvement",
							"section": "Improvement",
							"hidden": false
						}
					]
				}
			}
		],
		"@semantic-release/npm",
		[
			"@semantic-release/git",
			{
				"assets": [
					"ios/ColuConsumerRn/Info.plist",
					"ios/ColuConsumerRn.xcodeproj/project.pbxproj",
					"docs/CHANGELOG.md",
					"android/app/build.gradle",
					"package.json"
				],
				"message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
			}
		],
		[
			"@semantic-release/github",
			{
				"successComment": false,
				"failComment": false
			}
		]
	]
}
